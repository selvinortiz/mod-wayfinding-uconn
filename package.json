{
  "name": "uncraft",
  "version": "1.0.0",
  "description": "Craft CMS starter template with a minimalist Webpack config that includes Vue and Tailwind",
  "main": "webpack.config.js",
  "scripts": {
    "start": "webpack --watch --progress --mode=development --hide-modules",
    "start:sync": "CRAFT_HOST_URL={http://yourlocalsite.test} webpack --watch --progress --mode=development --hide-modules",
    "build": "NODE_ENV=production webpack --mode=production"
  },
  "keywords": [
    "craftcms",
    "boilerplace",
    "tailwind",
    "webpack",
    "vue"
  ],
  "author": "Selvin Ortiz <selvin@selvin.co>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "autoprefixer": "^9.6.0",
    "axios": "^0.19.0",
    "babel-loader": "^8.0.6",
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "^2.2.2",
    "core-js": "^3.1.4",
    "css-loader": "^3.0.0",
    "extract-loader": "^3.1.0",
    "file-loader": "^4.0.0",
    "html-loader": "^0.5.5",
    "node-sass": "^4.12.0",
    "postcss-cli": "^6.1.2",
    "postcss-loader": "^3.0.0",
    "regenerator-runtime": "^0.13.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "tailwindcss": "^1.0.4",
    "vue": "^2.6.10",
    "vue-loader": "^15.7.0",
    "vue-router": "^3.0.7",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "vuex": "^3.1.1",
    "webpack": "^4.35.2",
    "webpack-cli": "^3.3.5"
