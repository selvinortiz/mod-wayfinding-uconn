{
  "name": "modcore",
  "version": "1.0.0",
  "description": "A Craft CMS starter template with a minimalist Webpack config that includes Vue and Tailwind",
  "main": "webpack.config.js",
  "scripts": {
    "start": "webpack --watch --progress --mode=development --hide-modules",
    "start:sync": "cross-env CRAFT_HOST_URL=http://mod-wayfinding-core.test webpack --watch --progress --mode=development --hide-modules",
    "start:sync:so": "cross-env CRAFT_HOST_URL=https://modcore.test webpack --watch --progress --mode=development --hide-modules",
    "start:sync:pl": "cross-env CRAFT_HOST_URL=http://modcore.test webpack --watch --progress --mode=development --hide-modules",
    "build": "NODE_ENV=production webpack --mode=production"
  },
  "keywords": [
    "craftcms",
    "boilerplace",
    "tailwind",
    "webpack",
    "vue"
  ],
  "author": "Selvin Ortiz <selvin@modcreativeinc.com>",
  "license": "MIT",
